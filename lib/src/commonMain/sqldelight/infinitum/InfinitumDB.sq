CREATE TABLE infinitum_teste (
	name TEXT NOT NULL,
	number INTEGER NOT NULL
);

CREATE INDEX infinitum_number ON infinitum_teste(number);

INSERT INTO infinitum_teste (number, name)
VALUES (10, 'Tiaguito');

selectAll:
SELECT *
FROM infinitum_teste;

insert:
INSERT INTO infinitum_teste (number, name)
VALUES (?, ?);
